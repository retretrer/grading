{
  "name": "Udacity Front End Feedback",
  "short_name": "Udacity Feedback",
  "version": "0.0.9",
  "manifest_version": 2,
  "description": "Get feedback on the structure of any page.",
  "homepage_url": "http://github.com/udacity/frontend-grading-engine",
  "icons": {
    "16": "icons/udacity-invert16.png",
    "48": "icons/udacity-invert48.png",
    "128": "icons/udacity-invert128.png"
  },
  "default_locale": "en",
  "options_page": "src/options/index.html",
  "browser_action": {
    "default_icon": "icons/udacity-invert48.png",
    "default_title": "Udacity Feedback",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "src/js/inject.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "src/js/*.js",
    "src/js/libs/*.js",
    "fonts/*.css",
    "fonts/*/*",
    "src/templates/*.html"
  ]
}